    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Override": {
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%LogDir%\\Astrana\\log-{Date}.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "shared": true
        }
      },
      //{
      //  "Name": "PostgreSQL",

  "Logging": {
    "LogLevel": {
      "default": "Information"
    },
    "EventLog": {
      "LogLevel": {
        "default": "Information"
      }
    }
  },
  "Ssl": {
    "Port": "443",
    "CertificateName": "astrana.pem",
    "Password": "astrana"
  },
  "DatabaseProvider": "MSSqlServer",
  "ConnectionStrings": {

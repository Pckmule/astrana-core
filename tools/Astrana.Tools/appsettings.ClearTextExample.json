      //    "tableName": "Logs",
      //    "schemaName": "logs",
      //    "needAutoCreateTable": true
      //  }
      //},
      {
        "Name": "MySQL",
        "Args": {
          "connectionString": "Server=localhost;Port=3306;Uid=astranau;Pwd=I@m5ecure!!;Database=astrana",
          "tableName": "Logs",
          "schemaName": "logs",
          "storeTimestampInUtc": true
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=localhost,1433;Database=astrana;User Id=astranau;Password=I@m5ecure!!;Encrypt=False;TrustServerCertificate=True;",
          "sinkOptionsSection": {
            "tableName": "Logs",
            "schemaName": "logs",
            "autoCreateSqlTable": true,
            "batchPostingLimit": 1
          },
          "restrictedToMinimumLevel": "Warning",
          "columnOptionsSection": {}
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ]
  },
  "Columns": {
    "message": "RenderedMessageColumnWriter",
    "message_template": "MessageTemplateColumnWriter",
    "level": {
      "Name": "LevelColumnWriter",
      "Args": {
        "renderAsText": true,
        "dbType": "Varchar"
      }
    },
    "raise_date": "TimestampColumnWriter",
    "exception": "ExceptionColumnWriter",
    "properties": "LogEventSerializedColumnWriter",
    "props_test": {
      "Name": "PropertiesColumnWriter",
      "Args": {
        "dbType": "Json"
      }
    },
    "machine_name": {
      "Name": "SinglePropertyColumnWriter",
      "Args": {
        "propertyName": "MachineName",
        "writeMethod": "Raw"
      }
    }
  },

<?xml version="1.0" encoding="utf-8"?>
<!--
	This configuration file is used by the Astrana Installation tools to install and configure an Astrana instance.

	Sections:

		Peer
		
		WebServer
			Host
				Provider [Self | IIS | NGINX | Kestrel]
				
				RevereseProxy
					Provider [IIS | NGINX]
				
			SslCertificate
				Provider [Self | Vendor]
		
		Database
			Provider [MSSQLServer | Postgresql | MySQL]
			
		Application
-->
<Astrana>
	<Peer>
		<Language>en</Language>
		<RegionCountry>au</RegionCountry>
		<TimeZone>au</TimeZone>
		<FirstName></FirstName>
		<LastName></LastName>
		<EmailAddress></EmailAddress>
		<PhoneCode></PhoneCode>
		<PhoneNumber></PhoneNumber>
	</Peer>
	<WebServer>
		<ContainerName>Astrana</ContainerName>
		<Host>
			<Provider>iis</Provider>
			<Domain>astrana.local</Domain>
			<Port>80</Port>
			<RevereseProxy enabled="false">
				<Provider></Provider>
			</RevereseProxy>
		</Host>
		<SslCertificate>
			<Provider>Self</Provider>
			<Location></Location>
		</SslCertificate>
	</WebServer>
	<Database>
		<Provider>MSSqlServer</Provider>
		<Name>astrana2</Name>
		<HostAddress>localhost</HostAddress>
		<HostPort></HostPort>
		<UserName>astranau2</UserName>
		<Password>astranau</Password>
	</Database>
	<Application>
		<PackageArchiveName>astrana-1.0.0-win.zip</PackageArchiveName>
	</Application>
</Astrana>